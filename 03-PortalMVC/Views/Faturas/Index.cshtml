@model IEnumerable<_01_DAL.Fatura>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Data_Fatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empregado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Preco_Final)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Data_Fatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Empregado.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Preco_Final)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
